#include<stdio.h>
int main()
{
	int i,j,k,l,c=6,m;
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=i;j++)
		{
			printf(" ");
		}
		c=c-1;
		for(k=1;k<=c;k++)
		{
			printf("%d",k);
		}
		for(m=c;m>=1;m--)
		{
			if((m-1)!=0)
		     printf("%d",m-1);
	    }
		printf("\n");
		
	}
}



Output : 
123454321
 1234321
  12321
   121
    1
