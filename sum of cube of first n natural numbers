WHILE LOOP

#include<stdio.h>
int main()
{
	int i=1,num,sum=0;
	printf("Enter number of numbers:");
	scanf("%d",&num);
	while(i<=num)
	{
	sum=sum+(i*i*i);
	i++;
    }
    printf("%d",sum);
    return 0;
}


FOR LOOP

#include<stdio.h>
int main()
{
	int i,num,sum=0;
	printf("Enter number of numbers:");
	scanf("%d",&num);
	for(i=1;i<=num;i++)
	{
	sum=sum+(i*i*i);
    }
    printf("%d",sum);
    return 0;
}



